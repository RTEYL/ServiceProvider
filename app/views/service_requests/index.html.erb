<% @provider.service_requests.each do |req| %>
  <%= req.service.service_type %>
  <br>
  <%= form_for :service_request, url: provider_service_request_path(req.id, provider_id: @provider.id), method: 'patch' do |f| %>
    <%= f.label :completed %>
    <%= f.check_box :completed %>
    <br>
    <%= f.submit 'Complete'%>
  <% end %>
<% end %>